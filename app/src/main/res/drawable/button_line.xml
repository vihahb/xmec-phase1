<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:color="@color/login_button_start_press" android:width="1dp" />
            <solid android:color="@color/login_button_end_press"/>
            <corners android:radius="@dimen/size_2" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:color="@color/login_button_start" android:width="1dp" />
            <corners android:radius="@dimen/size_2" />
        </shape>
    </item>
</selector>